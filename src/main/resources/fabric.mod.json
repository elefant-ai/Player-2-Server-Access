{
  "schemaVersion": 1,
  "id": "bot-authentication",
  "version": "${version}",
  "name": "bot-authentication",
  "description": "Allows AI friends from Elefant to join your minecraft server!",
  "authors": [],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/bot-authentication/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.elefant.botAuthentication.client.BotAuthenticationClient"
    ],
    "main": [
      "com.elefant.botAuthentication.BotAuthentication"
    ]
  },
  "mixins": [
    {
      "config":"bot-authentication.mixins.json",
      "environment": "server"
    },
    {
      "config": "bot-authentication.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_3248": ["com/elefant/botAuthentication/ElefantLoginPackets"],
      "net/minecraft/class_2911": ["com/elefant/botAuthentication/ElefantLoginPackets"]

    }
  }
}
